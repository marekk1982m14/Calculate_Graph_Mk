#include <iostream>
#include <iomanip>
#include <cstdlib>
#include <math.h>


using namespace std;




int main()
{
	float a,b,c,d,e,f,g,h,i,j,k;
	float y;
        float m = 1.1101010101010111111;
	{

		a = 1.1212;
		b = a * 2.2222 + (b-a);
		c = b * 3.1001 + (c-b);
		d = c * 9.9999 + (d-c);
		e = d * 7.8788 + (e-d);
		f = e * 2.3433 + (f-e);
		g = f * 5.6555 + (g-f);
		h = g * 6.7777 + (h-g);
		i = h * 3.2223 + (i-h);
		j = i * 2.0101 + (j-i);
		k = j * 1.1011 + (k-j);

	}

		
			for(m;m<10000000000000;m++,a++,b++,c++,d++,e++,f++,g++,h++,i++,j++,k++)
			{

				m = sqrt( ( (b-a)*(b-a)+(c-b)*(c-b)+(d-c)*(d-c)+(e-d)*(e-d)+(f-e)*(f-e)+(g-f)*(g-f)+(h-g)*(h-g)+(i-h)*(i-h)+(j-i)*(j-i)+(k-j)*(k-j)  ) ) / ( (a+b+c+d+e+f+g+h+i+j+k)*0.1110101);
				cout << setprecision(80) <<"\t\tm3:\t" << m;

	if( m < 10.1010101010101110100 )
	{		
				for(a;a<10000;a++)
				{
					cout << "\ta:\t" << a;
						for(b;b<10000;b++)
						{
							cout << "b:" << b;
								for(c;c<10000;c++)
								{
									cout << "\tc:\t" << c;
										for(d;d<10000;d++)
										{
											cout << "\td:\t" << d;
												for(e;e<10000;e++)
												{
													cout << "\te:\t" << e;
														for(f;f<10000;f++)
														{
															cout << "\tf:\t" << f;
																for(g;g<10000;g++)
																{
																	cout << "\tg:\t" << g;
																						    for(h;h<10000;h++)
																						    {
																							    cout << "\th:\t" << h;
																							    	for(i;i<10000;i++)
				    {
					cout << "\ti:\t" << i;
			    			for(j;j<10000;j++)
						{
							cout << "\tj:\t" << j;
								for(k;k<10000000000000;k++)
								{
									cout << "\tk:\t" << k;
								}
						}
				    }
																						    }
																}
														}
												}
										}
								}
						}
				}
			}

		}

}
